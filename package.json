{
	"name": "fix-on-save-toggle",
	"displayName": "Fix on Save Toggle",
	"publisher": "adrianwilczynski",
	"icon": "img/icon.png",
	"description": "Toggle formatting, fixing and import organizing on save.",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/AdrianWilczynski/FixOnSaveToggle.git"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.toggleFixOnSave",
				"title": "Toggle Fix on Save"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/lodash": "^4.14.138",
		"lodash": "^4.17.15"
	}
}
